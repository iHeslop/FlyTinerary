{
  "name": "react-globe.gl",
  "version": "2.23.3",
  "description": "React component for Globe Data Visualization using ThreeJS/WebGL",
  "license": "MIT",
  "type": "module",
  "unpkg": "dist/react-globe.gl.min.js",
  "main": "dist/react-globe.gl.mjs",
  "module": "dist/react-globe.gl.mjs",
