  "author": {
    "name": "Vasco Asturiano",
    "url": "https://github.com/vasturiano"
  },
  "bugs": {
    "url": "https://github.com/vasturiano/react-globe.gl/issues"
  },
  "homepage": "https://github.com/vasturiano/react-globe.gl",
